{
  "name": "react-untabbable",
  "version": "1.0.3",
  "description": "An extremely simple utility to make a container and all its children untabbable, but still focusable.",
  "source": "src/index.ts",
  "main": "untabbable.cjs.js",
  "module": "untabbable.esm.js",
  "scripts": {
    "build": "yarn run clean && rollup -c",
    "clean": "rm -rf coverage untabbable.*.js untabbable.*.ts",
    "test": "jest test --coverage",
    "test:watch": "jest test --watch",
    "publish": "npm publish",
    "prepublishOnly": "yarn test && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdiogo/react-untabbable.git"
  },
  "keywords": [
    "react",
    "hook",
    "focus",
    "tab",
    "tabbable",
    "untabbable"
  ],
  "author": "Diogo Ferreira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fdiogo/react-untabbable/issues"
  },
  "homepage": "https://github.com/fdiogo/react-untabbable#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-typescript": "^4.1.1",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.34",
    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "react": "16.8",
    "react-dom": "16.8",
    "rollup": "^2.8.2",
    "rollup-plugin-dts": "^1.4.1",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "tabbable": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^16.8"
  }
}
